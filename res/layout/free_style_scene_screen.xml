<?xml version="1.0" encoding="utf-8"?>
<com.miui.home.launcher.upsidescene.SceneScreen
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <include
        layout="@layout/free_style_edit_bar" />
    <com.miui.home.launcher.upsidescene.SceneContentView
        android:layout_gravity="top"
        android:id="@id/sceneContent"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">
        <include
            android:id="@id/backgroundScreen"
            layout="@layout/free_style_fixed_screen" />
        <com.miui.home.launcher.upsidescene.ScrollableScreen
            android:id="@id/scrollableScreen"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">
            <com.miui.home.launcher.upsidescene.FreeLayout
                android:id="@id/freeLayout"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent" />
        </com.miui.home.launcher.upsidescene.ScrollableScreen>
        <include
            android:id="@id/dockScreen"
            layout="@layout/free_style_fixed_screen" />
        <include
            android:id="@id/foregroundScreen"
            layout="@layout/free_style_fixed_screen" />
    </com.miui.home.launcher.upsidescene.SceneContentView>
</com.miui.home.launcher.upsidescene.SceneScreen>